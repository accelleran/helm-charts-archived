apiVersion: v1
kind: Pod
metadata:
  name: dash-grafana-test
  labels:
    app: dash-grafana
    chart: "grafana-4.4.0"
    heritage: "Helm"
    release: "dash-grafana"
  annotations:
    "helm.sh/hook": test-success
  namespace: test
spec:
  serviceAccountName: dash-grafana-test
  initContainers:
    - name: test-framework
      image: "dduportal/bats:0.4.0"
      command:
      - "bash"
      - "-c"
      - |
        set -ex
        # copy bats to tools dir
        cp -R /usr/local/libexec/ /tools/bats/
      volumeMounts:
      - mountPath: /tools
        name: tools
  imagePullSecrets:
    - name: accelleran-secret
  containers:
    - name: dash-grafana-test
      image: "dduportal/bats:0.4.0"
      command: ["/tools/bats/bats", "-t", "/tests/run.sh"]
      volumeMounts:
        - mountPath: /tests
          name: tests
          readOnly: true
        - mountPath: /tools
          name: tools
  volumes:
  - name: tests
    configMap:
      name: dash-grafana-test
  - name: tools
    emptyDir: {}
  restartPolicy: Never
